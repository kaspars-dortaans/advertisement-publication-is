{
  "name": "advertisementwebsite.client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen:api-client": " cd gen-scripts/nswag && nswag.bat",
    "gen:locales": "tsx ./gen-scripts/build-locales.ts",
    "gen:constants": "tsx ./gen-scripts/build-constants.ts",
    "gen:all": "run-p gen:api-client gen:constants gen:locales"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@tsconfig/node22": "^22.0.1",
    "@vee-validate/yup": "^4.13.2",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.7.2",
    "nswag": "^14.0.8",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.5",
    "tailwindcss-primeui": "^0.4.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.29",
    "vue-advanced-cropper": "^2.8.9",
    "vue-router": "^4.3.3",
    "vuedraggable": "^4.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^20.17.30",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.0.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.3",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.2.2",
    "vue-tsc": "^2.0.21"
  }
}
